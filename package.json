{
  "name": "telar-mongo",
  "version": "1.0.0",
  "description": "Telar mongo in JavaScript.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json --maxWorkers=10",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/telarpress/telar-mongo-js.git"
  },
  "keywords": [
    "telar",
    "mongo"
  ],
  "author": "Amir Movahedi <amir.gholzam@live.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/telarpress/telar-mongo-js/issues"
  },
  "homepage": "https://github.com/telarpress/telar-mongo-js#readme",
  "dependencies": {
    "mongo-heartbeat": "^2.0.1",
    "mongodb": "^3.6.2",
    "telar-core": "^1.0.13",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/jest": "^26.0.15",
    "@types/mongodb": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3"
  },
  "files": [
    "lib"
  ]
}
